{
   "jsc":{
      "loose":true,
      "target":"es2020",
      "parser":{
         "syntax":"typescript",
         "decorators":true,
         "privateMethod":true,
         "decoratorsBeforeExport":true,
         "topLevelAwait":true,
         "importMeta":true
      },
      "transform":{
         "legacyDecorator":true,
         "decoratorMetadata":true
      },
      "keepClassNames":true,
      "externalHelpers":false
   },
   "module":{
      "type":"commonjs",
      "noInterop":true,
      "strict":true
   }
}